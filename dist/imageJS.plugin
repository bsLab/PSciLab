                                    break;
                                }
                            }
                        }
                    }

                    if (!skip) {
                        ret_nonmax.push({
                            x: corners[i].x,
                            y: corners[i].y,
                            score: scores[i]
                        });
                    }
                }

                return ret_nonmax;
