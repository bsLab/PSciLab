                        for (; corners[point_above].y < pos.y && corners[point_above].x < pos.x - 1; point_above++) {}


                        for (j = point_above; corners[j].y < pos.y && corners[j].x <= pos.x + 1; j++) {
                            var x = corners[j].x;
