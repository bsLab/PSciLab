                            for (; point_below < corners.length && corners[point_below].y == pos.y + 1 && corners[point_below].x < pos.x - 1; point_below++) {}

                            for (j = point_below; j < corners.length && corners[j].y == pos.y + 1 && corners[j].x <= pos.x + 1; j++) {
