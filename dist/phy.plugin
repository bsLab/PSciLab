// typeof @kind  = 'displacement','length','force','mass'
// The mass property returns the force acting on each mass!
simuPhy.prototype.measure = function (kind) {
  if (this.scene && this.objects && this.objects.measure)
    return this.objects.measure(kind);
  else if (this.scene && this.objects && this.objects.dims && this.objects.masses) {
    var i,j,k,p;   // X,Y,Z
    var dims=this.objects.dims;
    var X=dims[0],Y=dims[1],Z=dims[2];
    var masses = this.objects.masses;
    var tensor = [];
    for(i=0;i<X;i++) {
      var row=[];
        for(j=0;j<Y;j++) {
      	  var col=[];
          for(k=0;k<Z;k++) {
            var o={};
