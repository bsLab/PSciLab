// Are two shapes equal?
window.prototype.equal = function (shape1,shape2,geo) {
  if (shape1.shape!=shape2.shape) return false;
  if (!this.rtree.equal(shape1.bbox,shape2.bbox)) return false;
  if (geo) return true;
  return false;
}
