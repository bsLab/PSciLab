       function(srcWid, dstWid, srcX, srcY) {
           return [ 'CxSLLSS', [ 40, 4, srcWid, dstWid, srcX, srcY ] ];
       },
       function(buf, sameScreen)
       {
           var res = buf.unpack('Lss');
           var ext = {};
           ext.child = res[0];
           ext.destX = res[1];
           ext.destY = res[2];
           ext.sameScreen = sameScreen;
