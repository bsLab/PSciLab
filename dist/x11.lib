        {
            X.seq_num++;
//            captureStack();
            X.pack_stream.pack('CCSL', [ext.majorOpcode, 7, 2, window ]);
            X.replies[X.seq_num] = [
                function(buf, opt) {
                    return opt;
                },
                cb
            ];
            X.pack_stream.flush();
        }

