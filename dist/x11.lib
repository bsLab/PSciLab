       },
       function(buf, revertTo)
       {
           return {
               focus : buf.unpack('L')[0],
               revertTo : revertTo
           };
       }
   ],

   OpenFont: [
       function(name,fid) {
           var padded = xutil.padded_string(name);
           return ['CxSLSxxa', [45, 3+padded.length/4, fid, name.length, padded] ];
       }    
   ],

