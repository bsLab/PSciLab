type  : tiff
convert : GRAY8
index : $1($data)
schema : 
  index : int unique = $row   // $index($row)
  type : int = 1
  data : blob = $typedarray($data)
  meta  :
    file  : text = $file
    dataspace : array = $dataspace($data)
    datatype  : text = 'Uint8'
    rows  : int  = $rows($data) # $dims[0]($data)
    cols  : int  = $cols($data) # $dims[1]($data)
    _type : text = json
# Dataset API wrapper table
